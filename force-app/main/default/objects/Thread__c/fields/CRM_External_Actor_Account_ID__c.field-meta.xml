<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CRM_External_Actor_Account_ID__c</fullName>
    <description>Formula field to define which party is the external. Field is populated with the Salesforce account ID.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(CRM_From__r.Contact.Id )),
			 CRM_From__r.Contact.Account.Id,
			CRM_Recipient__r.Contact.Account.Id
			)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>External Actor Account ID</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
